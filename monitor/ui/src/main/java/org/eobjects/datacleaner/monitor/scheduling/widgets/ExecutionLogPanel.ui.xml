<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' xmlns:dc="urn:import:org.eobjects.datacleaner.monitor.shared.widgets">

	<g:HTMLPanel styleName="ExecutionLogPanel">

		<div class="FormRow">
			<span class="FormKey">Status:</span>
			<g:Label styleName="FormValue" ui:field="statusLabel" />
		</div>

		<div class="FormRow">
			<span class="FormKey">Begin time:</span>
			<g:Label styleName="FormValue" ui:field="beginTimeLabel" />
		</div>

		<div class="FormRow">
			<span class="FormKey">End time:</span>
			<g:Label styleName="FormValue" ui:field="endTimeLabel" />
		</div>


		<div class="FormRow">
			<span class="FormKey">Trigger:</span>
			<g:Label styleName="FormValue" ui:field="triggerLabel" />
		</div>
		
		<div class="FormRow">
			<span class="FormKey">Triggered by:</span>
			<g:Label styleName="FormValue" ui:field="triggeredByLabel" />
		</div>

		<div class="FormRow" style="margin-top: 25px;">
			<span>Log output:</span>
			<div class="LogOutputPanel">
				<g:Label styleName="LogOutputLabel" ui:field="logOutputLabel" />
			</div>
		</div>
		
		<div class="FormRow" style="margin-top: 10px;">
			<span class="FormKey">Result:</span>
			<div class="FormValue">
				<dc:LoadingIndicator ui:field="loadingIndicator" />
				<g:Anchor styleName="FormValue" ui:field="resultAnchor" />
			</div>
		</div>

	</g:HTMLPanel>
</ui:UiBinder>