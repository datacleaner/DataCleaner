<assembly>
	<id>extension</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/</outputDirectory>
			<useProjectArtifact>true</useProjectArtifact>
			<unpack>true</unpack>
			<scope>runtime</scope>
			<includes>
				<include>org.eobjects.datacleaner:*</include>
				<include>org.elasticsearch:*</include>
				<include>org.apache.lucene:lucene-core</include>
				<include>org.apache.lucene:lucene-analyzers-common</include>
			</includes>
		</dependencySet>
	</dependencySets>
</assembly>
