<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
	<id>zip-assembly</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>true</includeBaseDirectory>
	<baseDirectory></baseDirectory>
	<fileSets>
		<fileSet>
			<outputDirectory>DataCleaner</outputDirectory>
			<directory>src/assembly/bin</directory>
			<fileMode>0755</fileMode>
			<includes>
				<include>*.sh</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory>DataCleaner</outputDirectory>
			<directory>src/assembly/bin</directory>
			<includes>
				<include>*.cmd</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory>DataCleaner</outputDirectory>
			<directory>target</directory>
			<includes>
				<include>*.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory>DataCleaner/lib</outputDirectory>
			<directory>target/lib</directory>
			<includes>
				<include>*.jar</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory>DataCleaner</outputDirectory>
			<directory>../..</directory>
			<includes>
				<include>COPYING.txt</include>
				<include>NOTICE.txt</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>