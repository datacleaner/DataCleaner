<?xml version="1.0" encoding="UTF-8"?>
<configuration xmlns="http://eobjects.org/analyzerbeans/configuration/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<datastore-catalog>
		<jdbc-datastore name="orderdb" description="DataCleaner example database">
			<url>jdbc:hsqldb:res:orderdb;readonly=true</url>
			<driver>org.hsqldb.jdbcDriver</driver>
			<username>SA</username>
			<password></password>
		</jdbc-datastore>
	</datastore-catalog>

	<reference-data-catalog>
		<dictionaries>
		</dictionaries>
		<synonym-catalogs>
		</synonym-catalogs>
	</reference-data-catalog>

	<custom-taskrunner
		class-name="org.eobjects.datacleaner.monitor.configuration.SharedTaskRunner" />

	<custom-descriptor-provider
		class-name="org.eobjects.datacleaner.monitor.configuration.SharedDescriptorProvider" />

</configuration>