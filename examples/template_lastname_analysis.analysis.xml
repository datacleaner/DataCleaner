<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<job xmlns="http://eobjects.org/analyzerbeans/job/1.0">
    <job-metadata>
        <job-description>Created with DataCleaner 2.0 (BETA)</job-description>
        <updated-date>2011-02-09+01:00</updated-date>
    </job-metadata>
    <source>
        <data-context ref="datastore"/>
        <columns>
            <column path="table.Lastname column" id="col_0"/>
        </columns>
    </source>
    <transformation>
        <transformer name="Match lastname dictionaries">
            <descriptor ref="Dictionary matcher"/>
            <properties>
                <property value="[English lastnames,Spanish lastnames,Eastern lastnames]" name="Dictionaries"/>
            </properties>
            <input ref="col_0"/>
            <output id="col_2" name="Matches 'English lastnames'"/>
            <output id="col_3" name="Matches 'Spanish lastnames'"/>
            <output id="col_4" name="Matches 'Eastern lastnames'"/>
        </transformer>
        <transformer name="Extract ending">
            <descriptor ref="JavaScript transformer"/>
            <properties>
                <property value="function eval() {&#xA;  var str = values&amp;#91;0&amp;#93;;&#xA;  var len = str.length;&#xA;  var ending = str.substring(len-3&amp;#44; len);&#xA;  return ending;&#xA;}&#xA;&#xA;eval();" name="Source code"/>
            </properties>
            <input ref="col_0"/>
            <output id="col_5" name="Lastname's ending"/>
        </transformer>
        <transformer name="Extract length">
            <descriptor ref="String length"/>
            <properties/>
            <input ref="col_0"/>
            <output id="col_1" name="Lastname's length"/>
        </transformer>
    </transformation>
    <analysis>
        <analyzer name="Boolean analyzer (matches)">
            <descriptor ref="Boolean analyzer"/>
            <properties/>
            <input ref="col_2"/>
            <input ref="col_3"/>
            <input ref="col_4"/>
        </analyzer>
        <analyzer name="Value distribution (ending)">
            <descriptor ref="Value distribution"/>
            <properties>
                <property value="true" name="Record unique values"/>
            </properties>
            <input ref="col_5"/>
        </analyzer>
        <analyzer>
            <descriptor ref="Pattern finder"/>
            <properties>
                <property value="true" name="Discriminate text case"/>
                <property value="false" name="Discriminate negative numbers"/>
                <property value="true" name="Discriminate decimals"/>
                <property value="false" name="Enable mixed tokens"/>
                <property value="false" name="Ignore repeated spaces"/>
                <property value="false" name="Upper case patterns expand in size"/>
                <property value="false" name="Lower case patterns expand in size"/>
                <property value="&amp;#44;" name="Decimal separator"/>
                <property value="." name="Thousands separator"/>
                <property value="-" name="Minus sign"/>
            </properties>
            <input ref="col_0"/>
        </analyzer>
        <analyzer name="Number analyzer (length)">
            <descriptor ref="Number analyzer"/>
            <properties/>
            <input ref="col_1"/>
        </analyzer>
    </analysis>
</job>
